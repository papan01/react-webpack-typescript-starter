{
  "name": "frontend-starter",
  "version": "1.0.0",
  "description": "Webpack + TypeScript + React + Babel + ESLint",
  "private": true,
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist ./node_modules package-lock.json",
    "prod": "npm run build -- --env.mode production",
    "dev": "npm run build -- --env.mode development",
    "start:prod": "webpack-dev-server --open --env.mode production",
    "start:dev": "webpack-dev-server --open --env.mode development",
    "lint": "eslint \"src/**/*.{tsx, ts, js, jsx}\"",
    "lint:fix":"npm run lint -- --fix"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papan01/web-app-starter.git"
  },
  "keywords": [
    "Webpack",
    "TypeScript",
    "React",
    "Babel"
  ],
  "author": "Louis_Peng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/papan01/web-app-starter/issues"
  },
  "homepage": "https://github.com/papan01/web-app-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
